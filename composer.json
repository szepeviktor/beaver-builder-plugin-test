{
    "name": "szepeviktor/beaver-builder-plugin-test",
    "description": "Static Analysis for Beaver Builder",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpstan/phpstan-shim": "^0.11.5",
        "giacocorsiglia/wordpress-stubs": "^5.1"
    },
    "autoload-dev": {
        "psr-4": {
            "PHPStan\\WordPress\\": "tests/phpstan/"
        },
        "classmap": [
            "classes/",
            "extensions/",
            "includes/",
            "modules/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "classmap-authoritative": true
    }
}
